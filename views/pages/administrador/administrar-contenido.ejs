                                    <a v-else @click="mostrarEditarDescripcion"><i class="fas fa-edit"></i></a>
                                </div>
                            </div>

                        </div>




                    </template>
                    <!-- columna derecha -->

                    <div class="col-sm-2 col-derecha" :style="{backgroundColor: objetoSeleccionado.color}">
